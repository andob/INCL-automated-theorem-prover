digraph {
    a [label="P ⊃ Q"];
    b [label="¬P"];
    c [label="Q"];
    a -> b; a -> c;
}

digraph {
    a [label="P ≡ Q"];
    b [label="P"];
    c [label="Q"];
    d [label="¬P"];
    e [label="¬Q"];
    a -> b; b -> c; a -> d; d -> e;
}

digraph {
    a [label="¬(P ⊃ Q)"];
    b [label="P"];
    c [label="¬Q"];
    a -> b; b -> c;
}

digraph {
    a [label="¬(P ≡ Q)"];
    b [label="P"];
    c [label="Q"];
    d [label="¬P"];
    e [label="¬Q"];
    a -> b; b -> e; a -> d; d -> c;
}
