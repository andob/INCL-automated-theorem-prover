digraph {
    a [label="(constant domain)\n∃x P[x], wᵢ, +"];
    b [label="P[a:x], wᵢ, +"];
    a -> b;
}

digraph {
    a [label="(variable domain)\n∃x P[x], wᵢ, +"];
    b [label="P[a:x], wᵢ, +"];
    c [label="𝔈a:x, wᵢ, +"];
    a -> b; b -> c;
}

digraph {
    a [label="¬∃x P[x], wᵢ, ±"];
    b [label="∀x ¬P[x], wᵢ, ±"];
    a -> b;
}
